echo AWCONSOLE~env~>| 
read c
	
if [ $c -eq "OUTPUT" ] 
do
  echo AWCONSOLE~env~>|OUTPUT~>| 
  read print
  echo $print
fi

if [ $c -eq "EXECUTE" ] 
do
  echo AWCONSOLE~env~>|EXECUTE~>| 
  read execute
  input=$execute
while IFS= read -r line
do

  if [ $doap -eq 1 ]
  do
    echo $line
  fi
  
  if [$line -eq "OUTPUT"]
  do
    doap=1
  fi
  
  
done < "$input"
fi
